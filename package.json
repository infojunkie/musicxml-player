    "@types/eslint": "^8.4.5",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/jest": "^29.2.4",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.6.3",
    "@types/verovio": "^3.11.0",
    "@types/webmidi": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-puppeteer": "^6.1.1",
    "http-serve": "^1.0.1",
    "jest": "^29.3.1",
    "jest-dev-server": "^6.1.1",
    "jest-mock": "^29.3.1",
    "jest-puppeteer": "^6.1.1",
    "prettier": "^2.7.1",
    "puppeteer": "^17.1.0",
    "rollup": "^3.5.0",
    "rollup-plugin-external-globals": "^0.7.1",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "build": "rollup --config ./rollup.config.mjs",
    "develop": "rollup --watch --config ./rollup.config.mjs",
    "demo": "http-serve -c0 -p8080 demo",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infojunkie/musicxml-player.git"
  },
  "keywords": [],
  "author": {
    "name": "Karim Ratib"
  },
